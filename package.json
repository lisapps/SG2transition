  "scripts": {
    "build": "NODE_ENV=production parcel build ./client/src/index.html --reporter @parcel/reporter-bundle-buddy",
    "staging": "NODE_ENV=staging parcel build ./client/src/index.html",
    "start": "node ./server/bin/www node server/app.js",
    "format": "prettier 'client/src/**/*.{js,jsx, html}'",
    "lint": "eslint \"**/*.{js,jsx}\" --quiet",
    "dev-react": "NODE_ENV=development parcel client/src/index.html",
    "dev-srv": "NODE_ENV=development nodemon app.js",
    "dev-scss": "node-sass client/styles/kingston.scss -o  client/public/static/  &&  node-sass client/styles/hyperx.scss -o  client/public/static/",
    "dev": "concurrently \"npm:dev-scss\"  \"npm:dev-srv\" \"npm:dev-react\" "
  },
  "staticFiles": {
    "staticPath": "client/public/static",
    "staticOutDir": "static",
    "watcherGlob": "**"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./setupTests.js"
  },
  "proxy": "http://localhost:3005",
